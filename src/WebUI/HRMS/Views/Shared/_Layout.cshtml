    </div>
    </footer>*@
    <script src="~/dha_theme/scripts/jquery-3.5.1.js" asp-append-version="true"></script>
    <script src="~/dha_theme/scripts/bootstrap-4.5.2.js" asp-append-version="true"></script>
    <script src="~/dha_theme/scripts/all.min.js" asp-append-version="true"></script>
    <script src="~/dha_theme/scripts/jquery.datatables.min.js" asp-append-version="true"></script>
    <script src="~/dha_theme/scripts/datatables.fixedcolumns.min.js" asp-append-version="true"></script>
    <script src="~/dha_theme/scripts/dt-javascript.js" asp-append-version="true"></script>
