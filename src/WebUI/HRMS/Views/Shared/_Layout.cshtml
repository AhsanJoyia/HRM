<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HRMS</title>

    <link href="~/dha_theme/bootstrap/bootstrap-4.5.2.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/dha_theme/css/all.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/dha_theme/css/custom.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/dha_theme/css/jquery.datatables.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/dha_theme/css/fixedcolumns.datatables.min.css" rel="stylesheet" asp-append-version="true" />

</head>
<body style="background: #f6f6f6">
    <partial name="_Header" />
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                @RenderBody()
            </div>
        </div>
    </div>
